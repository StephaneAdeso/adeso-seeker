{
  "name": "adeso-seeker",
  "displayName": "seeker",
  "description": "Free and opensource alternative to postman and thunderclient.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "seeker-sidebar",
          "icon": "assets/icon.svg",
          "title": "Seeker"
        }
      ]
    },
    "views": {
      "seeker-sidebar": [
        {
          "id": "seeker.sidebarView",
          "name": "Collections",
          "contextualTitle": "Collections",
          "type": "webview"
        },
        {
          "id": "seeker.environments-tree",
          "name": "Environments",
          "contextualTitle": "Environments",
          "type": "tree"
        },
        {
          "id": "seeker.history-tree",
          "name": "History",
          "contextualTitle": "History",
          "type": "tree"
        }
      ]
    },
    "commands": [
      {
        "command": "adeso-seeker.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --mode development --progress",
    "watch": "webpack --mode development --progress --watch",
    "package": "webpack --mode production ",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "mocha": "^10.2.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
